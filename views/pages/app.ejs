<!DOCTYPE html>
<html style='height:100%' lang="<%= language %>">
  <head>
    <meta name='viewport' content='width=device-width, initial-scale=1, shrink-to-fit=no' />
    <title><%= title %></title>

    <!--  css  -->
    <% overrideCss && overrideCss.forEach(function(css){%>
      <link rel="stylesheet" href="<%- css %>" />
    <% });%>
  </head>

  <body style='margin:0; height:100%; background-color: grey'>
    <div id="root"><%= container %></div>

    <!--  js  -->
    <% overrideJs && overrideJs.forEach(function(js){%>
      <script src="<%- js %>"></script>
    <% });%>
  </body>
</html>