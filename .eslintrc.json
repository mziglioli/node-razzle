{
  "extends": "plugin:react/recommended",
  "env": {
    "browser": true,
    "commonjs": true,
    "node": true,
    "es6": false
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module",
    "allowImportExportEverywhere": true
  },
  "rules": {
    "no-console": "off",
    "strict": ["error", "global"],
    "curly": "warn"
  }
}
